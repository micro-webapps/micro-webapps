{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "webapp-wordpress",
        "annotations": {
            "webconf-spec": "{\"virtualhost\": \"$mwa_vhost\",\"proxy\": { \"url\": \"http://webapp/$mwa_alias\", \"alias\": \"$mwa_alias\"}}"
        }
    },
    "spec": {
        "selector": {
            "app": "webapp-wordpress"
        },
        "ports": [
            {
                "name": "http-port",
                "protocol": "TCP",
                "port": 80,
                "targetPort": 80
            }
        ]
    }
}
