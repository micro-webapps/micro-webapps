{
    "kind": "Service",
    "apiVersion": "v1beta3",
    "metadata": {
        "name": "webapp-wordpress",
        "annotations": {
            "mwa_webconfig": "{\"virtualhost\": \"$mwa_vhost\",\"proxy_protocol\": \"http://\",\"proxy_alias\": \"$mwa_alias\",\"proxy_backend_alias\": \"/\"}"
        }
    },
    "spec": {
        "selector": {
            "name": "webapp-wordpress"
        },
        "ports": [
            {
                "name": "http-port",
                "protocol": "TCP",
                "port": 80,
                "targetPort": 80
            }
        ]
    }
}