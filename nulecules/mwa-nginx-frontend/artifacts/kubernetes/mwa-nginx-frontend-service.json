{
    "kind": "Service",
    "apiVersion": "v1beta3",
    "metadata": {
        "name": "nginx-frontend-service"
    },
    "spec": {
        "selector": {
            "name": "nginx-frontend"
        },
        "publicIPs": ["$publicip"],
        "ports": [
            {
                "name": "http-port",
                "protocol": "TCP",
                "port": 80,
                "targetPort": 80
            },
            {
                "name": "https-port",
                "protocol": "TCP",
                "port": 443,
                "targetPort": 443
            }
        ]
    }
}
